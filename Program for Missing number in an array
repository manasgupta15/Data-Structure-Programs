#include<stdio.h>

int main()
{
    int i,sum=0,missing;
    int n;
    printf("enter the size of array\n ");
    scanf("%d",&n);

    int array[n-1];
    printf("enter the elements in the array\n");
    for(i=0;i<n-1;i++)
    {
        scanf("%d",&array[i]);
        sum=sum+array[i];
    }
    missing=(n*(n+1))/2-sum;
    printf("missing number is= %d ",missing);

}
